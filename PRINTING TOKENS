/* C Program to print Tokens */
/* TO PRINT GIVE SENTANCE INTO WORDS */
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    char str[1000];
    gets(str);
    for(int i=0;str[i] != '\0';i++)
    {
        if(str[i] == ' ')
            printf("\n");
        else 
            printf("%c",str[i]);
    }
    return 0;
}
